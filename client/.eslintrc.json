{
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": ["angular"],
  "extends": ["eslint:recommended", "plugin:angular/johnpapa"],
  "rules": {
    "angular/controller-as": "off",
    "angular/file-name": "off",
    "no-undef": "off"
  }
}
